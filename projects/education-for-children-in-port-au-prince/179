				window.location.href='http://spicyfund.com/demo/home/login/aHR0cDovL3NwaWN5ZnVuZC5jb20vZGVtby9wcm9qZWN0cy9lZHVjYXRpb24tZm9yLWNoaWxkcmVuLWluLXBvcnQtYXUtcHJpbmNlLzE3OQ==';
				
				}
			else{
			document.getElementById("like").innerHTML=xmlhttp.responseText;}
			}
		  }
		xmlhttp.open("GET",strURL,true);
		xmlhttp.send();
    }
