	 <script type="text/javascript">
    function ajaxfollow_proj(id)
    {   
		var strURL="http://spicyfund.com/demo/project/follow_project/"+id;
		
		var xmlhttp;
		if (window.XMLHttpRequest)
		  {// code for IE7+, Firefox, Chrome, Opera, Safari
		  xmlhttp=new XMLHttpRequest();
		  
		  }
		else
		  {// code for IE6, IE5
		  xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
		  
		  }
		xmlhttp.onreadystatechange=function()
		  {
			 
		  if (xmlhttp.readyState==4 && xmlhttp.status==200)
			{	
			//alert(xmlhttp.responseText);
			if(xmlhttp.responseText=='login_failed'){
				window.location.href='http://spicyfund.com/demo/home/login/aHR0cDovL3NwaWN5ZnVuZC5jb20vZGVtby9oZWxwL2ZhcS9iYWNraW5nLWEtcHJvamVjdA==';
				
				}
			else{
			document.getElementById("follow").innerHTML=xmlhttp.responseText;}
			}
		  }
		xmlhttp.open("GET",strURL,true);
		xmlhttp.send();
    }
	 function ajaxunfollow_proj(id)
    {   
		var strURL="http://spicyfund.com/demo/project/unfollow_project/"+id;
		// alert(strURL);
		var xmlhttp;
		if (window.XMLHttpRequest)
		  {// code for IE7+, Firefox, Chrome, Opera, Safari
		  xmlhttp=new XMLHttpRequest();
		  
		  }
		else
		  {// code for IE6, IE5
		  xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
		  
		  }
		xmlhttp.onreadystatechange=function()
		  {
			 // alert('res'+xmlhttp.responseText);
		  if (xmlhttp.readyState==4 && xmlhttp.status==200)
			{	
			
			document.getElementById("follow").innerHTML=xmlhttp.responseText;
			}
		  }
		xmlhttp.open("GET",strURL,true);
		xmlhttp.send();
    }
	
	  function ajaxlike_proj(id)
     {   
